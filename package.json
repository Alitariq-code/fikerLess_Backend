{
  "name": "fikrless-backend",
  "version": "1.0.0",
  "description": "Fikrless Backend API",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "test:password-flows": "ts-node scripts/test-password-flows.ts",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "import:quotes": "ts-node scripts/import-quotes.ts",
    "import:audios": "ts-node scripts/import-audios.ts",
    "import:internships": "ts-node scripts/import-internships.ts",
    "create:admin": "ts-node scripts/create-admin.ts",
    "test:forum-is-user-post": "ts-node scripts/test-forum-is-user-post.ts",
    "create:dummy-users": "ts-node scripts/create-dummy-users.ts",
    "seed:achievements": "ts-node scripts/seed-achievements.ts",
    "test:achievements": "ts-node scripts/test-achievements.ts",
    "test:forum-comments": "ts-node scripts/test-forum-comments.ts",
    "test:forum-complete": "ts-node scripts/test-forum-complete.ts",
    "test:forum-admin": "ts-node scripts/test-forum-admin.ts",
    "add:forum-comments": "ts-node scripts/add-forum-test-comments.ts",
    "test:wellness-complete": "ts-node scripts/test-wellness-complete.ts",
    "test:complete-system": "ts-node scripts/test-complete-system.ts",
    "test:booking-availability": "ts-node scripts/test-booking-availability.ts",
    "test:booking-slots-sessions": "ts-node scripts/test-booking-slots-sessions.ts",
    "test:booking-complete-flow": "ts-node scripts/test-booking-complete-flow.ts",
    "test:booking-admin-sessions": "ts-node scripts/test-booking-admin-sessions.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/mongoose": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/throttler": "^6.5.0",
    "@types/multer": "^2.0.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "firebase-admin": "^12.0.0",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.6.0",
    "mongoose": "^7.5.0",
    "multer": "^2.0.2",
    "music-metadata": "^11.10.3",
    "nodemailer": "^6.9.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.9",
    "@types/passport-jwt": "^3.0.9",
    "@types/supertest": "^2.0.12",
    "@types/xlsx": "^0.0.35",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
